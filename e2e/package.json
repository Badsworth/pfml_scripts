    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "playwright-chromium": "^1.10.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3",
    "webpack": "^4.44.1"
  },
