{
  "name": "pfml-automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress",
    "cypress:open": "cypress open",
    "cypress:open:local": "E2E_ENVIRONMENT=local cypress open",
    "cypress:open:local:portal": "E2E_ENVIRONMENT=local cypress open --config integrationFolder=cypress/specs/stable/scenario",
    "cypress:run": "cypress run",
    "lint": "npm run lint:js -- --fix && npm run lint:ts",
    "lint:ci": "npm run lint:js -- --max-warnings=0 && npm run lint:ts",
    "lint:ts": "tsc --noEmit",
    "lint:js": "eslint src/ cypress/ test/ nerdpack/",
    "format": "prettier --write '**/*.{js,json,md,mdx,ts,tsx,scss,yaml,yml}'",
    "format-check": "prettier --check '**/*.{js,json,md,mdx,ts,tsx,scss,yaml,yml}'",
    "test:unit": "jest --config=jest.unit.json",
    "test:integration": "jest --config=jest.integration.json --testPathIgnorePatterns=./test/integration/dor_upload.[jt]s",
    "test:integration:ci": "jest --config=jest.integration.json --reporters=default --reporters=./test/reporter.js",
    "cli": "ts-node src/cli.ts",
    "pilot3:gen": "ts-node src/cli.ts simulation generate -f ./src/simulation/scenarios/pilot3.ts -d ./data/pilot3",
    "pilot3:sim": "ts-node src/cli.ts simulation run -d ./data/pilot3",
    "pilot4:gen": "ts-node src/cli.ts simulation generate -f ./src/simulation/scenarios/pilot4.ts -d ./data/pilot4",
    "pilot4:sim": "ts-node src/cli.ts simulation run -d ./data/pilot4",
    "generate:api": "npx @spec2ts/openapi-client ../api/openapi.yaml -o src/_api.ts",
    "artillery:dev": "npm run artillery:bundle && artillery run -e basic dist/development.yml",
    "artillery:run": "E2E_ENVIRONMENT=performance npm run artillery:bundle && artillery run -e performance dist/development.yml",
    "artillery:bundle": "rm -rf ./dist && webpack --config=webpack.artillery.config.ts",
    "newrelic:generate-fineos-dashboards": "ts-node src/scripts/2021-07-19-generate-nr-api-dashboards.ts"
  },
