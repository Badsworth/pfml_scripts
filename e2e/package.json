{
  "name": "pfml-automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "npm run lint:js -- --fix && npm run lint:ts",
    "lint:ci": "npm run lint:js && npm run lint:ts",
    "lint:ts": "tsc --noEmit",
    "lint:js": "eslint src/ cypress/ test/",
    "test": "jest",
    "cli": "ts-node src/cli.ts",
    "pilot3:gen": "ts-node src/cli.ts simulation generate -f ./src/simulation/scenarios/pilot3.ts -d ./data/pilot3",
    "pilot3:sim": "ts-node src/cli.ts simulation run -d ./data/pilot3",
    "generate:api": "npx @spec2ts/openapi-client ../api/openapi.yaml -o src/api.ts",
    "flood:all": "element run src/flood/index.perf.ts",
    "flood:plan": "element plan src/flood/index.perf.ts",
    "flood:api": "element run src/flood/tests/api.perf.ts --no-headless",
    "flood:fineos": "element run src/flood/tests/fineos.perf.ts --no-headless",
    "flood:fineosDeny": "element run src/flood/tests/fineosDeny.perf.ts --no-headless"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@bahmutov/cypress-extends": "^1.0.0",
    "@cypress/webpack-preprocessor": "^5.4.2",
    "@types/cypress-cucumber-preprocessor": "^1.14.1",
    "@types/faker": "^4.1.12",
    "@types/form-data": "^2.5.0",
    "@types/merge2": "^1.3.0",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-loader": "^8.1.0",
    "cypress-cucumber-preprocessor": "^2.5.4",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.3.0",
    "pdf2json": "^1.2.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^3.9.6",
    "webpack": "^4.44.1"
  },
  "dependencies": {
    "@flood/element": "^1.3.7",
    "@flood/element-cli": "^1.3.7",
    "amazon-cognito-identity-js": "^4.4.0",
    "cross-fetch": "^3.0.6",
    "csv-stringify": "^5.5.1",
    "cypress": "^4.11.0",
    "cypress-file-upload": "^4.0.7",
    "date-fns": "^2.16.1",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "form-data": "^3.0.0",
    "merge2": "^1.4.1",
    "node-fetch": "^2.6.1",
    "p-retry": "^4.2.0",
    "pdf-lib": "^1.9.0",
    "typescript": "^3.9.6",
    "winston": "^3.3.3",
    "yargs": "^16.0.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "commonPath": "cypress/integration/endtoend/common"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.[jt]s?(x)"
    ]
  },
  "element": {
    "testMatch": [
      "*/flood/*.[tj]s"
    ]
  }
}
