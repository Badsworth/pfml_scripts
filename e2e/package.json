    "node-fetch": "^2.6.1",
    "p-retry": "^4.2.0",
    "pdf-lib": "^1.11.1",
    "pdf-parse": "^1.1.1",
    "ssn": "^1.0.3",
    "streaming-iterables": "^5.0.4",
    "ts-jest": "^26.5.4",
