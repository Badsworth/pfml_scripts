{
  "name": "pfml-automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress",
    "cypress:open": "cypress open",
    "cypress:open:local": "E2E_ENVIRONMENT=local cypress open",
    "cypress:open:local:portal": "E2E_ENVIRONMENT=local cypress open --config integrationFolder=cypress/specs/stable/scenario",
    "cypress:run": "cypress run",
    "lint": "npm run lint:js -- --fix && npm run lint:format && npm run lint:ts",
    "lint:ci": "npm run lint:js && npm run lint:ts",
    "lint:ts": "tsc --noEmit",
    "lint:js": "eslint src/ cypress/ test/",
    "lint:format": "prettier src/ cypress/ test/ --write --loglevel silent",
    "test:unit": "jest --config=jest.unit.json",
    "test:integration": "jest --config=jest.integration.json",
    "cli": "ts-node src/cli.ts",
    "pilot3:gen": "ts-node src/cli.ts simulation generate -f ./src/simulation/scenarios/pilot3.ts -d ./data/pilot3",
    "pilot3:sim": "ts-node src/cli.ts simulation run -d ./data/pilot3",
    "pilot4:gen": "ts-node src/cli.ts simulation generate -f ./src/simulation/scenarios/pilot4.ts -d ./data/pilot4",
    "pilot4:sim": "ts-node src/cli.ts simulation run -d ./data/pilot4",
    "generate:api": "npx @spec2ts/openapi-client ../api/openapi.yaml -o src/_api.ts",
    "flood:all": "element run src/flood/index.perf.ts",
    "flood:plan": "element plan src/flood/index.perf.ts",
    "flood:compile": "element compile src/flood/index.perf.ts",
    "flood:PortalClaimSubmit": "element run src/flood/scenarios/PortalClaimSubmit.perf.ts --no-headless",
    "flood:FineosClaimSubmit": "element run src/flood/scenarios/FineosClaimSubmit.perf.ts --no-headless",
    "flood:SavilinxAgent": "element run src/flood/scenarios/SavilinxAgent.perf.ts --no-headless",
    "flood:LeaveAdminSelfReg": "element run src/flood/scenarios/LeaveAdminSelfReg.perf.ts --no-headless",
    "artillery:dev": "npm run artillery:bundle&& artillery run -e test -q dist/spec.yml",
    "artillery:run": "E2E_ENVIRONMENT=performance npm run artillery:bundle && artillery run -e performance dist/spec.yml",
    "artillery:bundle": "rm -rf ./dist && webpack --config=webpack.artillery.config.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@testing-library/cypress": "^8.0.0",
    "@types/copy-webpack-plugin": "^6.4.2",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.22",
    "@types/js-yaml": "^4.0.0",
    "@types/multipipe": "^3.0.0",
    "@types/ndjson": "^2.0.0",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/pdf-parse": "^1.1.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^4.41.28",
    "@types/zip-webpack-plugin": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.4.1",
    "cypress-grep": "^2.5.3",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-unused-imports": "^1.1.2",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "playwright-chromium": "^1.12.2",
    "prettier": "^2.3.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.2",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "@flood/element": "^1.5.4",
    "@flood/element-cli": "^1.5.4",
    "abort-controller": "^3.0.0",
    "amazon-cognito-identity-js": "^5.0.6",
    "artillery": "^1.7.6",
    "aws-sdk": "^2.935.0",
    "cross-fetch": "^3.0.6",
    "csv": "^5.5.0",
    "csv-stringify": "^5.6.2",
    "cypress": "^8.3.0",
    "cypress-file-upload": "^5.0.8",
    "date-fns": "^2.16.1",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "encoding": "^0.1.13",
    "enquirer": "^2.3.6",
    "faker": "^4.1.0",
    "form-data": "^4.0.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "js-yaml": "^4.0.0",
    "JSONStream": "^1.3.5",
    "multipipe": "^4.0.0",
    "ndjson": "^2.0.0",
    "node-fetch": "^2.6.1",
    "p-retry": "^4.2.0",
    "pdf-lib": "^1.11.1",
    "pdf-parse": "^1.1.1",
    "ssn": "^1.0.3",
    "streaming-iterables": "^5.0.4",
    "ts-jest": "^26.5.4",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "yargs": "^17.0.1"
  },
  "element": {
    "testMatch": [
      "*/flood/*.[tj]s"
    ]
  },
  "volta": {
    "node": "14.17.3",
    "npm": "6.14.13"
  }
}
