{
  "name": "pfml-automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress",
    "cypress:open": "cypress open",
    "cypress:open:local": "E2E_ENVIRONMENT=local cypress open",
    "cypress:run": "cypress run",
    "lint": "npm run lint:js -- --fix && npm run lint:ts",
    "lint:ci": "npm run lint:js && npm run lint:ts",
    "lint:ts": "tsc --noEmit",
    "lint:js": "eslint src/ cypress/ test/",
    "test": "jest",
    "cli": "ts-node src/cli.ts",
    "pilot3:gen": "ts-node src/cli.ts simulation generate -f ./src/simulation/scenarios/pilot3.ts -d ./data/pilot3",
    "pilot3:sim": "ts-node src/cli.ts simulation run -d ./data/pilot3",
    "pilot4:gen": "ts-node src/cli.ts simulation generate -f ./src/simulation/scenarios/pilot4.ts -d ./data/pilot4",
    "pilot4:sim": "ts-node src/cli.ts simulation run -d ./data/pilot4",
    "generate:api": "npx @spec2ts/openapi-client ../api/openapi.yaml -o src/api.ts",
    "flood:all": "element run src/flood/index.perf.ts",
    "flood:plan": "element plan src/flood/index.perf.ts",
    "flood:build": "cd scripts && makeFloodBundle.sh",
    "flood:compile": "element compile src/flood/index.perf.ts",
    "flood:PortalClaimSubmit": "element run src/flood/scenarios/PortalClaimSubmit.perf.ts --no-headless",
    "flood:FineosClaimSubmit": "element run src/flood/scenarios/FineosClaimSubmit.perf.ts --no-headless",
    "flood:SavilinxAgent": "element run src/flood/scenarios/SavilinxAgent.perf.ts --no-headless",
    "flood:LeaveAdminSelfReg": "element run src/flood/scenarios/LeaveAdminSelfReg.perf.ts --no-headless"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@types/cypress-cucumber-preprocessor": "^3.0.0",
    "@types/faker": "^4.1.12",
    "@types/form-data": "^2.5.0",
    "@types/multipipe": "^3.0.0",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/pdf-parse": "^1.1.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-loader": "^8.1.0",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3",
    "webpack": "^4.44.1"
  },
  "dependencies": {
    "@flood/element": "^1.3.9",
    "@flood/element-cli": "^1.3.9",
    "JSONStream": "^1.3.5",
    "abort-controller": "^3.0.0",
    "amazon-cognito-identity-js": "^4.4.0",
    "cross-fetch": "^3.0.6",
    "csv-stringify": "^5.5.1",
    "cypress": "^6.2.0",
    "cypress-file-upload": "^4.1.1",
    "date-fns": "^2.16.1",
    "delay": "^4.3.0",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "form-data": "^3.0.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "multipipe": "^4.0.0",
    "node-fetch": "^2.6.1",
    "p-retry": "^4.2.0",
    "pdf-lib": "^1.11.1",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^5.3.1",
    "ssn": "^1.0.3",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "yargs": "^16.0.3"
  },
  "peerDependencies": {
    "puppeteer": "^2.1.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "commonPath": "cypress/tests/common"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.[jt]s?(x)"
    ]
  },
  "element": {
    "testMatch": [
      "*/flood/*.[tj]s"
    ]
  }
}
