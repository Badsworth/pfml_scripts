    "@babel/preset-typescript": "^7.10.4",
    "@cypress/webpack-preprocessor": "^5.7.0",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.22",
    "@types/js-yaml": "^4.0.0",
    "@types/multipipe": "^3.0.0",
    "@types/ndjson": "^2.0.0",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/pdf-parse": "^1.1.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-loader": "^8.1.0",
