{
  "name": "mass_pfml",
  "version": "1.0.0",
  "description": "Mass PFML Portal",
  "main": "index.js",
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "scripts": {
    "analyze-bundle": "ANALYZE_BUNDLE=true next build",
    "dev": "NEXT_TELEMETRY_DISABLED=1 next",
    "dev:local": "NEXT_TELEMETRY_DISABLED=1 BUILD_ENV=local next",
    "docs": "start-storybook -p 6006 -c ./storybook -s ./public",
    "docs:build": "build-storybook -c ./storybook -s ./public -o out-storybook",
    "docs:smoke-test": "npm run docs -- --ci --smoke-test --quiet",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build && next export",
    "lint": "npm run lint:ci -- --fix",
    "lint:ci": "eslint --ext .js,.ts,.tsx ./",
    "format": "prettier --write '**/*.{js,json,md,mdx,ts,tsx,scss,yaml,yml}'",
    "format-check": "prettier --check '**/*.{js,json,md,mdx,ts,tsx,scss,yaml,yml}'",
    "generate-claims-page-stories": "node ./bin/generate-claims-page-stories.js",
    "start": "next start",
    "test": "NODE_ENV=test jest",
    "test:ci:src": "npm test -- --ci --coverage --coverage-reporters=lcov --coverage-reporters=text --testPathIgnorePatterns=tests/storybook/",
    "test:ci:storybook": "npm test -- --ci tests/storybook/",
    "pretest:ci:storybook": "npm run generate-claims-page-stories",
    "test:update-snapshots": "npm test -- -u",
    "test:watch": "npm test -- --watch",
    "ts:check": "tsc --noEmit",
    "predocs": "npm run generate-claims-page-stories",
    "predocs:build": "npm run generate-claims-page-stories"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.0.1",
    "@massds/mayflower-assets": "^11.0.2",
    "@massds/mayflower-react": "^11.0.2",
    "assert": "^2.0.0",
    "classnames": "^2.3.1",
    "compressorjs": "^1.0.7",
    "core-js": "^3.12.1",
    "dayjs": "^1.10.7",
    "dayjs-business-time": "^1.0.4",
    "downloadjs": "^1.4.7",
    "i18next": "^20.6.1",
    "js-cookie": "^3.0.0",
    "lodash": "^4.17.21",
    "mockdate": "^3.0.5",
    "postcss": "^8.4.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.15",
    "react-use": "^17.2.4",
    "uswds": "^2.13.1",
    "xstate": "^4.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@faker-js/faker": "^6.0.0-alpha.6",
    "@next/bundle-analyzer": "^12.0.2",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-docs": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addons": "^6.4.17",
    "@storybook/react": "^6.3.6",
    "@storybook/testing-react": "^0.0.22",
    "@storybook/theming": "^6.4.17",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest-axe": "^3.5.3",
    "@types/lodash": "^4.14.172",
    "@types/new-relic-browser": "^0.1118.2",
    "@types/react": "^17.0.30",
    "@types/react-helmet": "^6.1.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@babel/eslint-parser": "^7.17.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^8.10.0",
    "eslint-config-nava": "^11.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsdoc": "^37.9.6",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "eslint-plugin-testing-library": "^5.0.5",
    "eslint-plugin-todo-plz": "^1.2.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "jest": "^27.3.1",
    "jest-axe": "^5.0.1",
    "mermaid": "^8.13.9",
    "next": "^12.1.0",
    "next-images": "^1.8.2",
    "next-transpile-modules": "^9.0.0",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.3.2",
    "react-is": "^17.0.2",
    "readdirp": "^3.6.0",
    "recursive-readdir-sync": "^1.0.6",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.1",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  }
}
