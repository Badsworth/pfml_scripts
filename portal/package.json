{
  "name": "mass_pfml",
  "version": "1.0.0",
  "description": "Mass PFML Portal",
  "main": "index.js",
  "scripts": {
    "analyze-bundle": "ANALYZE_BUNDLE=true next build",
    "dev": "next",
    "docs": "start-storybook -p 6006 -c ./storybook -s ./public --docs",
    "docs:build": "build-storybook -c ./storybook -s ./public -o out-storybook --docs",
    "docs:smoke-test": "npm run docs -- --ci --smoke-test --quiet",
    "build": "next build && next export",
    "build:test": "BUILD_ENV=test npm run build",
    "build:stage": "BUILD_ENV=stage npm run build",
    "build:prod": "BUILD_ENV=prod npm run build",
    "start": "next start",
    "test": "NODE_ENV=test jest --ci --coverage",
    "test:ci": "npm test --  --coverage-reporters=lcov --coverage-reporters=text",
    "test:e2e": "PORT=3000 jest --config jest-e2e.config.js",
    "pretest:e2e": "echo 'ðŸ¤– Preparing end-to-end tests. This expects the site to be already be running on port 3000'",
    "test:e2e:ci": "JEST_PUPPETEER_CI=true npm run test:e2e",
    "pretest:e2e:ci": "npm run build",
    "test:e2e:watch": "npm run test:e2e -- --watch",
    "test:update-snapshots": "jest -u",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-amplify/auth": "^3.3.3",
    "@massds/mayflower-assets": "^0.2.1",
    "@massds/mayflower-react": "^10.0.0-alpha.2",
    "assert": "^2.0.0",
    "aws-amplify": "^3.0.22",
    "aws-amplify-react": "^4.1.21",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "i18next": "^19.5.6",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.19",
    "luxon": "^1.24.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "uswds": "^2.7.1",
    "xstate": "^4.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@next/bundle-analyzer": "^9.5.1",
    "@storybook/addon-docs": "^6.0.0-rc.0",
    "@storybook/react": "^6.0.0-rc.0",
    "@xstate/test": "^0.4.0",
    "@zeit/next-source-maps": "0.0.3",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "jest": "^26.0.1",
    "jest-puppeteer": "^4.4.0",
    "next": "^9.5.1",
    "next-images": "^1.4.1",
    "puppeteer": "^5.0.0",
    "react-is": "^16.13.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "state-machine-cat": "^7.0.6"
  }
}
