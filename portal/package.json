{
  "name": "mass_pfml",
  "version": "1.0.0",
  "description": "Mass PFML Portal",
  "main": "index.js",
  "scripts": {
    "analyze-bundle": "ANALYZE_BUNDLE=true next build",
    "dev": "next",
    "docs": "start-storybook -p 6006 -c ./storybook -s ./public --docs",
    "docs:build": "build-storybook -c ./storybook -s ./public -o out-storybook --docs",
    "docs:smoke-test": "npm run docs -- --ci --smoke-test --quiet",
    "build": "next build && next export",
    "build:test": "BUILD_ENV=test npm run build",
    "build:stage": "BUILD_ENV=stage npm run build",
    "build:prod": "BUILD_ENV=prod npm run build",
    "build:performance": "BUILD_ENV=performance npm run build",
    "build:training": "BUILD_ENV=training npm run build",
    "generate-claims-page-stories": "node ./bin/generate-claims-page-stories.js",
    "generate-playroom-components": "node ./bin/generate-playroom-components.js",
    "playroom": "playroom start",
    "playroom:build": "playroom build",
    "start": "next start",
    "test": "NODE_ENV=test jest --ci --coverage",
    "test:ci": "npm test --  --coverage-reporters=lcov --coverage-reporters=text",
    "predocs": "npm run generate-claims-page-stories",
    "predocs:build": "npm run generate-claims-page-stories",
    "preplayroom:build": "npm run generate-playroom-components",
    "preplayroom": "npm run generate-playroom-components",
    "test:update-snapshots": "jest -u",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-amplify/auth": "^3.4.10",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@massds/mayflower-assets": "^10.1.0",
    "@massds/mayflower-react": "^10.1.0",
    "assert": "^2.0.0",
    "classnames": "^2.2.6",
    "core-js": "^3.7.0",
    "downloadjs": "^1.4.7",
    "i18next": "^19.8.3",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "react-use": "^15.3.4",
    "uswds": "^2.9.0",
    "xstate": "^4.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@next/bundle-analyzer": "^10.0.1",
    "@storybook/addon-docs": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@xstate/test": "^0.4.1",
    "@zeit/next-source-maps": "0.0.3",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^6.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.6.3",
    "next": "^10.0.1",
    "next-images": "^1.6.2",
    "next-transpile-modules": "^4.1.0",
    "playroom": "^0.22.3",
    "react-is": "^16.13.1",
    "readdirp": "^3.5.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "state-machine-cat": "^7.0.13"
  }
}
