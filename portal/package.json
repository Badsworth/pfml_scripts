{
  "name": "mass_pfml",
  "version": "1.0.0",
  "description": "Mass PFML Portal",
  "main": "index.js",
  "engines": {
    "node": ">=15",
    "npm": ">=7"
  },
  "scripts": {
    "analyze-bundle": "ANALYZE_BUNDLE=true next build",
    "dev": "NEXT_TELEMETRY_DISABLED=1 next",
    "docs": "start-storybook -p 6006 -c ./storybook -s ./public --docs",
    "docs:build": "build-storybook -c ./storybook -s ./public -o out-storybook --docs",
    "docs:smoke-test": "npm run docs -- --ci --smoke-test --quiet",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build && next export",
    "build:test": "BUILD_ENV=test npm run build",
    "build:stage": "BUILD_ENV=stage npm run build",
    "build:prod": "BUILD_ENV=prod npm run build",
    "build:performance": "BUILD_ENV=performance npm run build",
    "build:training": "BUILD_ENV=training npm run build",
    "build:uat": "BUILD_ENV=uat npm run build",
    "build:breakfix": "BUILD_ENV=breakfix npm run build",
    "build:cps-preview": "BUILD_ENV=cps-preview npm run build",
    "lint": "npm run lint:ci -- --fix",
    "lint:ci": "eslint --ext .js,.ts,.tsx ./",
    "format": "prettier --write '**/*.{js,json,md,mdx,ts,tsx,scss,yaml,yml}'",
    "format-check": "prettier --check '**/*.{js,json,md,mdx,ts,tsx,scss,yaml,yml}'",
    "generate-claims-page-stories": "node ./bin/generate-claims-page-stories.js",
    "start": "next start",
    "test": "NODE_ENV=test jest --ci --coverage",
    "test:new": "NODE_ENV=test jest --ci --coverage --testPathPattern=.*/tests/",
    "test:old": "NODE_ENV=test jest --ci --coverage --testPathPattern=.*/tests-old/",
    "test:ci": "npm test --  --coverage-reporters=lcov --coverage-reporters=text",
    "test:debug": "NODE_ENV=test jest",
    "predocs": "npm run generate-claims-page-stories",
    "predocs:build": "npm run generate-claims-page-stories",
    "test:update-snapshots": "jest -u",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.0.1",
    "@massds/mayflower-assets": "^11.0.2",
    "@massds/mayflower-react": "^11.0.2",
    "assert": "^2.0.0",
    "classnames": "^2.3.1",
    "compressorjs": "^1.0.7",
    "core-js": "^3.12.1",
    "downloadjs": "^1.4.7",
    "i18next": "^20.2.2",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "postcss": "^8.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.15",
    "react-use": "^17.2.4",
    "uswds": "^2.12.0",
    "xstate": "^4.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@next/bundle-analyzer": "^10.2.3",
    "@storybook/addon-docs": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@xstate/test": "^0.4.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.29.0",
    "eslint-config-nava": "^10.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsdoc": "^35.4.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-todo-plz": "^1.2.0",
    "faker": "^5.5.3",
    "jest": "^27.0.3",
    "mermaid": "^8.10.1",
    "next": "^10.2.3",
    "next-images": "^1.7.0",
    "next-transpile-modules": "^7.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "react-is": "^16.13.1",
    "readdirp": "^3.6.0",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.1"
  }
}
