exports[`ConsentToDataSharing shows the correct content for non-employers 1`] = `
<form
  className="usa-form"
  onSubmit={[Function]}
>
  <Title>
    How this website uses your information
  </Title>
  <p
    className="margin-bottom-2"
  >
    The information you provide on this website will be used to administer the Paid Family and Medical Leave (PFML) program.
  </p>
  <Accordion>
    <AccordionItem
      heading="Applying for PFML"
    >
      <p>
        We need this information to:
      </p>
      <ul
        className="usa-list"
      >
        <li
          key="0"
        >
          Check your eligibility for coverage
        </li>
        <li
          key="1"
        >
          Determine your benefit amount
        </li>
        <li
          key="2"
        >
          Give you the best service possible
        </li>
      </ul>
    </AccordionItem>
    <AccordionItem
      heading="How we use your data"
    >
      <p>
        We’ll keep your information private as required by law. As a part of the application process, we may check the information you give with other state agencies. We may share information related to your claim with your employer, health care provider(s), and contracted private partners.
      </p>
    </AccordionItem>
    <AccordionItem
      heading="Full user agreements"
    >
      <p>
        <Trans
          components={
            Object {
              "informed-consent-link": <a
                href="https://www.mass.gov/paidleave-informedconsent"
              />,
              "privacy-policy-link": <a
                href="https://www.mass.gov/privacypolicy"
              />,
            }
          }
          i18nKey="pages.userConsentToDataSharing.fullUserAgreementBody"
        />
      </p>
    </AccordionItem>
  </Accordion>
  <Alert
    noIcon={true}
    state="info"
  >
    By continuing, I am indicating that I have read and understood the above user agreements. I give the Department of Family and Medical Leave permission to collect, share, and use my information consistent with the terms of the agreements linked above.
  </Alert>
  <Button
    loading={false}
    type="submit"
  >
    Agree and continue
  </Button>
</form>
`;

